    <function name="guide" type="None" variable="">
        <parameters>
            <parameter name="Weight" type="Real" array="False"/>
            <parameter name="Height" type="Real" array="False"/>
            <parameter name="BMI" type="Real" array="False"/>
        </parameters>
        <body>
            <declare name="months" type="Real" array="False" size=""/>
            <declare name="need, need2, pro, good" type="Real" array="False" size=""/>
            <assign variable="pro" expression="Weight*2"/>
            <output expression="&quot;Your BMI is &quot;&amp;BMI" newline="True"/>
            <if expression="BMI&gt;=30">
                <then>
                    <output expression="&quot;I am sorry to inform you but your BMI shows that you are obese.fret not I shall guide you.loosing 3 kgs a month is healthy.so I shall suggest a plan on the same. &quot;" newline="True"/>
                    <output expression="&quot;Every day make sure u consume around &quot;&amp;Weight&amp;&quot;g of protein.The calorie in take must be in the range of 2200 to 2250 calories everday.&quot;" newline="True"/>
                    <output expression="&quot;As for the Excersice plan make sure You walk around 195000 steps everday.Since it would be difficult any heavy excersices, please restrain from doing any other than walking.&quot;" newline="True"/>
                    <assign variable="need" expression="30*((Height)^2)"/>
                    <assign variable="good" expression="Weight-need"/>
                    <assign variable="months" expression="good/3"/>
                    <output expression="&quot;Follow this routein for the next &quot;&amp;months&amp;&quot; months.&quot;" newline="True"/>
                    <breakpoint/>
                    <assign variable="need2" expression="25*((Height)^2)"/>
                    <assign variable="good" expression="need-need2"/>
                    <assign variable="months" expression="good/4"/>
                    <output expression="&quot;Congratulations!After you've done the above plan your BMI should be around 25.NOW you come under the category of overweight.&quot;" newline="True"/>
                    <output expression="&quot;Your BMI comes under the catagorie of over weight.Loosing around 4 kgs a month is healthy,so we shall plan the same.&quot;" newline="True"/>
                    <output expression="&quot;Every day make sure u consume around &quot;&amp;Weight&amp;&quot;g of protein.The calorie in take must be in the range of 2200 to 2250 calories everday.&quot;" newline="True"/>
                    <output expression="&quot;AS for the Excersice plan make sure You walk around 20000 steps everday.Since you come under over weight catogery a moderate excersice is a must.&quot;" newline="True"/>
                    <output expression="&quot;EXCERSICE PLAN:&#13;&#10;30 minutes of push ups&#13;&#10;and 30 min of sit ups&quot;" newline="True"/>
                    <output expression="&quot;This plan must be followed for the next &quot;&amp;months&amp;&quot; months.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="BMI&gt;25">
                        <then>
                            <assign variable="need" expression="25*((Height)^2)"/>
                            <assign variable="good" expression="Weight-need"/>
                            <assign variable="months" expression="good/4"/>
                            <output expression="&quot; I am really sorry to inform you,but your BMI shows that you are OVERWEIGHT.Fret now I shall help you loose weight.loosing around 4kgs a month is healthy.so we shall plan the same.&quot;" newline="True"/>
                            <output expression="&quot;Every day make sure u consume around &quot;&amp;Weight&amp;&quot;g of protein.The calorie in take must be in the range of 2200 to 2250 calories everday.&quot;" newline="True"/>
                            <output expression="&quot;As for the Excersice plan make sure You walk around 20000 steps everday.Since you come under over weight catogery a moderate excersice is a must.&quot;" newline="True"/>
                            <output expression="&quot;EXCERSICE PLAN:&#13;&#10;30 minutes of push ups&#13;&#10;and 30 min of sit ups&quot;" newline="True"/>
                            <output expression="&quot;This plan must be followed for the next &quot;&amp;months&amp;&quot; months.&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="BMI&gt;18">
                                <then>
                                    <output expression="&quot;Wow, I your BMI shows that you are perfect. You dont need my assistance at all.I am really happy to see you healthy.please maintain the same.&quot;" newline="True"/>
                                </then>
                                <else>
                                    <assign variable="need" expression="18*((Height)^2)"/>
                                    <assign variable="good" expression="need-Weight"/>
                                    <assign variable="months" expression="good/3"/>
                                    <output expression="&quot;I am really sorry to inform you but your BMI shows that you are under weight.&quot;" newline="True"/>
                                    <output expression="&quot;fret not I shall help you gain mass.lets set our target as reaching BMI 18!&quot;" newline="True"/>
                                    <output expression="&quot;Our target is to gain 3 kgs each month.Make sure You consume &quot;&amp;pro&amp;&quot;g of protein everyday, And consume 2800 calories daily&quot;" newline="True"/>
                                    <output expression="&quot;Though you are under weight,Excersiceing daily is a must.make sure u spend 10 min daily on push ups to strengthen your arms and 10 min on sit-ups.&quot;" newline="True"/>
                                    <output expression="&quot;Please follow the plan for next &quot;&amp;months&amp;&quot;months.&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="h" type="Real" variable="height">
        <parameters>
            <parameter name="rname" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="age, HeightInput, mistake" type="Integer" array="False" size=""/>
            <declare name="Height" type="Real" array="False" size=""/>
            <declare name="HeightUnits" type="String" array="False" size=""/>
            <assign variable="mistake" expression="1"/>
            <declare name="iinches, inchesportion" type="Real" array="False" size=""/>
            <while expression="mistake==1">
                <output expression="rname &amp; &quot; can you tell me what units would you like to give for height (feet/cm/m)&quot;" newline="True"/>
                <comment text="height will be taken in different units and converted into meters for furthur usage"/>
                <input variable="HeightUnits"/>
                <if expression="Len(HeightUnits)==4">
                    <then>
                        <output expression="&quot;Ooh &quot; &amp; rname &amp; &quot; you've selected feet units,could u please enter your height in feet, and the the inches portion of height.&quot;" newline="True"/>
                        <input variable="HeightInput"/>
                        <assign variable="iinches" expression="heightInput*12"/>
                        <output expression="&quot;Now the inches portion please-&quot;" newline="True"/>
                        <input variable="inchesportion"/>
                        <assign variable="iinches" expression="iinches+inchesportion"/>
                        <assign variable="Height" expression="iinches*2.57"/>
                        <assign variable="Height" expression="Height/100"/>
                        <assign variable="mistake" expression="0"/>
                    </then>
                    <else>
                        <if expression="Len(HeightUnits)==2">
                            <then>
                                <output expression="&quot;Ooh &quot; &amp; rname &amp; &quot; you selected centemeter units.Could you please enter your height in centimeters.&quot;" newline="True"/>
                                <input variable="HeightInput"/>
                                <assign variable="Height" expression="HeightInput*0.01"/>
                                <assign variable="mistake" expression="0"/>
                            </then>
                            <else>
                                <if expression="len(HeightUnits)==1">
                                    <then>
                                        <output expression="&quot;Ooh &quot; &amp; rname &amp; &quot; you selected meter units.Could you please enter your height in meters.&quot;" newline="True"/>
                                        <input variable="Height"/>
                                        <assign variable="mistake" expression="0"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;You selected the wrong units could you please re-enter.&quot;" newline="True"/>
                                        <assign variable="mistake" expression="1"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </while>
        </body>
    </function>
    <function name="multi" type="None" variable="">
        <parameters>
            <parameter name="rname" type="String" array="False"/>
            <parameter name="age" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="plan, ForLoop, Quantity, SearchTemporary, NewFoodCalories, FoodCalories, FoodStorage, cl, f, net, kilo, FoodNumericalInput, IntFoodName, SpotFilling" type="Integer" array="False" size=""/>
            <declare name="FoodAlert, FoodDesision, FoodName, i, h, FoodInput" type="String" array="False" size=""/>
            <declare name="Food" type="String" array="True" size="30"/>
