                <assign variable="Food[1]" expression="&quot;milk&quot;"/>
                <assign variable="Food[2]" expression="&quot;tea&quot;"/>
                <assign variable="Food[3]" expression="&quot;coffee&quot;"/>
                <assign variable="Food[4]" expression="&quot;bread&quot;"/>
                <assign variable="Food[5]" expression="&quot;banana&quot;"/>
                <assign variable="Food[6]" expression="&quot;white rice&quot;"/>
                <assign variable="Food[7]" expression="&quot;dosa&quot;"/>
                <assign variable="Food[8]" expression="&quot;roti&quot;"/>
                <assign variable="Food[9]" expression="&quot;sandwitch&quot;"/>
                <assign variable="Food[10]" expression="&quot;salmon&quot;"/>
                <assign variable="Food[11]" expression="&quot;burger&quot;"/>
                <assign variable="Food[12]" expression="&quot;chicken biryani&quot;"/>
                <assign variable="Food[13]" expression="&quot;steak&quot;"/>
                <assign variable="SpotFilling" expression="14"/>
                <do expression="FoodAlert==&quot;no&quot;">
                    <do expression="FoodDesision=&quot;yes&quot;">
                        <assign variable="ItemNo[0]" expression="78"/>
                        <assign variable="ItemNo[1]" expression="87"/>
                        <assign variable="ItemNo[2]" expression="55"/>
                        <assign variable="ItemNo[3]" expression="53"/>
                        <assign variable="ItemNo[4]" expression="32"/>
                        <assign variable="ItemNo[5]" expression="89"/>
                        <assign variable="ItemNo[6]" expression="78"/>
                        <assign variable="ItemNo[7]" expression="113"/>
                        <assign variable="ItemNo[8]" expression="65"/>
                        <assign variable="ItemNo[9]" expression="226"/>
                        <assign variable="ItemNo[10]" expression="256"/>
                        <assign variable="ItemNo[11]" expression="295"/>
                        <assign variable="ItemNo[12]" expression="Random(100)+400"/>
                        <assign variable="ItemNo[13]" expression="300"/>
