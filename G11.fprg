            <parameter name="gender" type="Integer" array="False"/>
        </parameters>
        <body>
            <output expression="rname&amp;&quot; how long would you like the duration of analysis be? 1 day or multiple days?let me know by choosing between (1/multiple) &quot;" newline="True"/>
            <declare name="days" type="String" array="False" size=""/>
            <input variable="days"/>
            <if expression="days==&quot;1&quot;">
                <then>
                    <call expression="one(rname,age,gender)"/>
                </then>
                <else>
                    <call expression="multi(rname,age)"/>
                </else>
            </if>
        </body>
    </function>
