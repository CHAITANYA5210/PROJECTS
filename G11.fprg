    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Hi, I am MINI,A personal fitness analyzer.&quot;" newline="True"/>
            <declare name="gen, age" type="Integer" array="False" size=""/>
            <declare name="height, weight, BMI" type="Real" array="False" size=""/>
            <declare name="rname, developers" type="String" array="False" size=""/>
            <assign variable="rname" expression="NAME()"/>
            <assign variable="gen" expression="Gender(rname)"/>
            <if expression="gen==1">
                <then>
                    <assign variable="rname" expression="&quot;Mr.&quot;&amp; rname"/>
                    <output expression="&quot;Thank you for your response &quot;&amp; rname" newline="True"/>
                </then>
                <else>
                    <if expression="gen==2">
                        <then>
                            <assign variable="rname" expression="&quot;Ms.&quot;&amp;rname"/>
                            <output expression="&quot;Thank you for your response &quot;&amp; rname" newline="True"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <output expression="rname&amp;&quot; could you please tell me your age.&quot;" newline="True"/>
            <input variable="age"/>
            <assign variable="height" expression="h(rname)"/>
            <assign variable="weight" expression="w(rname)"/>
            <assign variable="BMI" expression="Weight/((Height)^2)"/>
            <output expression="rname&amp;&quot; your BMI is equal to &quot;&amp;Tofixed(BMI,2)" newline="True"/>
            <call expression="analysis(rname,age,gen)"/>
            <output expression="&quot;Now the analysis is completed.&quot;" newline="True"/>
            <output expression="&quot;Would you like me to asssist you to maintain a fit body?&quot;" newline="True"/>
            <declare name="would" type="String" array="False" size=""/>
            <input variable="would"/>
            <if expression="would==&quot;yes&quot;">
                <then>
                    <output expression="&quot;I am really happy that you've choosen me as your assist.I'll do my best to support you.&quot;" newline="True"/>
                    <call expression="guide(Weight,Height,BMI)"/>
                </then>
                <else/>
            </if>
            <output expression="&quot;Would you like to know my developers?&quot;" newline="True"/>
            <input variable="developers"/>
            <if expression="developers==&quot;yes&quot;">
                <then>
                    <call expression="developers"/>
                </then>
                <else/>
            </if>
            <output expression="&quot;Thank you for using me.Please stay healthy!&quot;" newline="True"/>
        </body>
    </function>
    <function name="analysis" type="None" variable="">
